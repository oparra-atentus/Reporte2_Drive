<html>
	<head>
	
		<!-- BEGIN TIENE_METADATA -->
 		<meta http-equiv="X-UA-Compatible" content="IE=9"/>
 		<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
		<!-- END TIENE_METADATA -->
	
		<title>Atentus.com: {__sitio_titulo}</title>
		
		<!-- Estilos calendario y sprite de iconos-->
		<link rel="stylesheet" href="css/especiales.css" type="text/css"/>
	</head>
	<body>

		<table width="90%" align="center">
			<tr>
				<td style="padding: 4px 4px 4px 0px;"><i class="spriteImg spriteImg-header"></i></td>
			</tr>
			<tr>
				<td height="10"></td>
			</tr>
			<tr>
				<td>
					<table width="100%">
						<tr>
							<td height="40" style="padding: 4px; background-color:#626262;" class="txtBlanco15b">SIMBOLOG√çA</td>
						</tr>
						<tr>
							<td height="4" style="background-color: #f47001;"></td>
						</tr>
					</table>
				</td>
			</tr>
			<tr>
				<td height="20"></td>
			</tr>
			<tr>
				<td>
					<table>
						<!-- BEGIN LISTA_CODIGOS -->
						<tr>
							<td class="{__codigo_class}">
								<table>
									<tr>
										<td width="80" height="25" bgcolor="#{__codigo_color}" align="center"><i class="sprite sprite-{__codigo_icono}"></i></td>
										
									</tr>
								</table>
							</td>
							<td class="{__codigo_class}">
								<table>
									<tr>
										<td height="25">
											<span class="txtNegro14b">{__codigo_nombre}</span>
											<span class="txtGris10b">(cod. {__codigo_id})</span>
										</td>
									</tr>
									<tr>
										<td class="txtGris12" height="20">{__codigo_descripcion}&nbsp;</td>
									</tr>
								</table>
							</td>
						</tr>
						<!-- END LISTA_CODIGOS -->
					</table>
				</td>
			</tr>
			<tr>
				<td height="20"></td>
			</tr>
			<tr>
				<td>
					<table width="100%">
						<tr>
							<td align="center" class="txtNegro14b">E-mail: soporte@atentus.com</td>
						</tr>
						<tr>
							<td align="center" class="txtGrisClaro10b">
								www.atentus.com
							</td>
						</tr>
						<tr>
							<td align="center" style="border-top: solid 1px #626262;">
								<span class="txtNegro16">&copy;</span>
								<span class="txtNaranjo16">atentus</span>
							</td>
						</tr>
					</table>
				</td>
			</tr>
