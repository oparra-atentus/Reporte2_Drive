<table width="100%" style="border-spacing: 0px; border-collapse: collapse;">
	<tr>
		<td>
			<table width="100%" style="border-spacing: 0px; border-collapse: collapse;">
				<tr>
					<td height="40" style="padding: 4px 4px 4px 4px; background-color:#f47001;">
						<span style="color: #ffffff; font-family: Trebuchet MS, Verdana, sans-serif; font-size: 15px; font-weight: bold;">{__reporte_titulo}</span><br>
						<span style="color: #ffffff; font-family: Trebuchet MS, Verdana, sans-serif; font-size: 13px; font-weight: bold;">{__objetivo_nombre}</span>
					</td>
					<td style="padding: 4px; background-color:#626262; color: #ffffff; font-family: Trebuchet MS, Verdana, sans-serif; font-size: 13px; font-weight: bold;">{__fecha_inicio} al {__fecha_termino}</td>
				</tr>
				<tr>
					<td height="4" style="background-color: #f47001;"></td>
					<td height="4" style="background-color: #f47001;"></td>
				</tr>
			</table>
		</td>
	</tr>
	<!-- BEGIN LISTA_ITEMS -->
	<tr>
		<td style="padding: 20px;">
			<table width="100%" cellspacing="0" border="0" cellpadding="0">
				<tr>
					<td style="color: #000000; font-family: Trebuchet MS, Verdana, sans-serif; font-size: 16px;">{__item_orden}. {__item_titulo}</td>
				</tr>
				<tr>
					<td height="15"></td>
				</tr>
				<tr>
					<td style="display: block;">{__item_contenido}</td>
				</tr>
			</table>
		</td>
	</tr>
	<tr>
		<td height="20"></td>
	</tr>
	<!-- END LISTA_ITEMS -->
</table>